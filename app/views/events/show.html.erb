<div class="row medium-8 large-7 columns content">
  <p id="notice"><%= notice %></p>

  <p>
    <strong>Name:</strong>
    <%= @event.name %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= @event.date %>
  </p>

  <p>
    <strong>User:</strong>
    <%= @event.user_id %>
  </p>

  <p>
    <strong>Place:</strong>
    <%= @event.place_id %>
  </p>

  <%= link_to 'Edit', edit_event_path(@event) %> |
  <%= link_to 'Back', events_path %>
</div>
